<html lang="en">
  <head>
    <title>Imagewatermark</title>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    />
    <link
        rel="stylesheet"
        href="/App.css"
      />
  </head>
  <body>
    <header class="header-global">
      <div>
        <nav id="navbar-main" class="navbar-main nav-align custom-toggler navbar-transparent headroom navbar navbar-expand-lg bg-light" style="background-color:#FF363F !important;">
          <a href="/" class="navbar-brand"><img alt="flookup" width="100" height="50" src="assets/logo.png">
          </a>
          <button type="button" class="navbar-toggler">
            <span class="navbar-toggler-icon"></span>
          </button>
        </nav>
        <br>
      </div>
    </header>
      <div class="container">
          <h1 class="text-center">
              Imagewatermark
          </h1>
          <form action="/imagewatermark" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <input type="file" name="file" id="" required class="form-control" >
            </div>
            <div class="form-group">
              <input type="file" name="image" id="" required class="form-control" >
          </div>
          <div class="form-group">
                <button class="btn btn-danger btn-block">
                    PUT IMAGE WATERMARK
                </button>
            </div>
        </form>
      </div>
</body>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</html>